{
  "name": "Extensible Test Extension", 
  "short_name": "Extensible Test Extension", 
  "icons": {
      "256": "images/icon-256.png", 
      "128": "images/icon-128.png", 
      "48": "images/icon-48.png",
      "32": "images/icon-32.png", 
      "16": "images/icon-16.png"
  }, 
  "description": "Minimalist extension for testing extensions for other extensions.", 
  "background": {
    "service_worker": "js/app/background_loader.js"
  }, 
  "version": "0.16", 
  "manifest_version": 3, 
  "action": {
    "default_icon": {
      "256": "images/icon-256.png", 
      "128": "images/icon-128.png", 
      "48": "images/icon-48.png",
      "32": "images/icon-32.png", 
      "16": "images/icon-16.png"
	}    
  }, 
  "permissions": [
  	"scripting",
    "storage", 
    "alarms",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "modules": [
  	"modules/hello",
  	"modules/cookies",
  	"modules/screenshot"
  ]
}
